{
  "name": "premium-weather-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "description": "Premium weather app with dark/light themes, animated SVG icons, and comprehensive forecasting",
  "keywords": [
    "weather",
    "react",
    "typescript",
    "theme",
    "forecast",
    "animated-icons"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npx tsc -b && npx vite build",
    "build:ci": "npx tsc -b && npx vite build",
    "build:ultra": "npx vite build --mode production",
    "build:deps": "echo 'Build dependencies check complete'",
    "build:fix-rollup": "node scripts/fix-rollup-deps.cjs",
    "build:fallback": "npx esbuild src/main.tsx --bundle --outdir=dist --format=esm --platform=browser --external:react --external:react-dom",
    "build:safe": "npm run build:fix-rollup && npm run build || npm run build:fallback",
    "fix:rollup": "node scripts/fix-rollup-deps.ts",
    "fix:rollup:clean": "node scripts/fix-rollup-deps.ts --clean",
    "fix:rollup:sh": "bash scripts/fix-rollup-deps.sh",
    "fix:rollup:sh-clean": "bash scripts/fix-rollup-deps.sh --clean",
    "build:production": "npm run build:deps && cross-env NODE_ENV=production npx tsc -b && npx vite build --mode production",
    "build:stats": "tsc -b && vite build --mode production && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
  "format": "prettier --write .",
  "format:check": "prettier --check .",
    "type-check": "npx tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "_comment_optimized_testing": "Optimized Testing Scripts",
    "test:sharded": "vitest run --config vitest.config.optimized.ts",
    "test:shard1": "VITEST_SHARD=1/4 vitest run --config vitest.config.optimized.ts 'src/utils/__tests__/**/*.test.{ts,tsx}'",
    "test:shard2": "VITEST_SHARD=2/4 vitest run --config vitest.config.optimized.ts 'src/components/**/*.test.{ts,tsx}'",
    "test:shard3": "VITEST_SHARD=3/4 vitest run --config vitest.config.optimized.ts 'src/__tests__/**/*.test.{ts,tsx}'",
    "test:shard4": "VITEST_SHARD=4/4 vitest run --config vitest.config.optimized.ts 'src/utils/__tests__/**/*.simple.test.{ts,tsx}'",
    "test:parallel": "concurrently \"npm run test:shard1\" \"npm run test:shard2\" \"npm run test:shard3\" \"npm run test:shard4\"",
    "test:fast": "vitest run --coverage=false --reporter=default",
    "test:performance": "time npm run test:parallel",
    "test:streamlined": "node scripts/streamlined-test-runner.cjs",
    "test:streamlined-fast": "cross-env TEST_MODE=fast node scripts/streamlined-test-runner.cjs",
    "test:streamlined-affected": "cross-env TEST_MODE=affected node scripts/streamlined-test-runner.cjs",
    "_comment_cicd_optimization": "CI/CD Optimization Scripts",
    "ci:optimize": "node scripts/ci-cd-optimizer.cjs",
    "ci:analyze": "npm run ci:optimize",
    "ci:fast": "npm run test:fast && npm run build",
    "ci:parallel": "npm run test:parallel && npm run build",
    "ci:quality": "npm run lint && npm run test:fast && npm run build",
    "ci:full": "npm run lint && npm run test:parallel && npm run build && npm run analyze:ci",
    "ci:setup": "node scripts/setup-cicd-optimization.ts",
    "_comment_quality": "Quality Assurance Scripts",
    "qa": "npm run lint && npm run test:coverage && npm run build",
    "qa:fast": "npm run lint && npm run build",
    "qa:fix": "npm run lint:fix && npm run test:coverage",
    "check": "npm run lint && npx tsc --noEmit",
    "fix": "npm run lint:fix && npm run check",
    "_comment_security_ide": "Security Analysis Scripts (IDE Integration)",
    "security:check": "node scripts/security-check.cjs",
    "security:pre-commit": "npm run security:check && npm run lint",
    "security:full": "npm run security:check && npm run lint && npm run test:fast",
    "_comment_reliability": "SonarCloud Reliability Improvement Scripts",
    "reliability:check": "node scripts/reliability-improver.cjs",
    "reliability:fix": "node scripts/reliability-fixer-clean.cjs",
    "reliability:cleanup": "node scripts/cleanup-imports.cjs",
    "reliability:full": "npm run reliability:fix && npm run reliability:cleanup && npm run reliability:check && npm run test",
    "_comment_maintenance": "Maintenance Scripts",
    "clean": "rm -rf dist coverage .vite node_modules/.cache",
    "clean:deps": "rm -rf node_modules package-lock.json && npm install",
    "clean:full": "npm run clean && npm run clean:deps",
    "fix:deps": "rm -rf node_modules package-lock.json && npm install --force",
    "reset": "npm run clean:full && npm run qa",
    "_comment_analysis": "Performance Analysis Scripts",
    "analyze": "npm run build && npm run analyze:bundle",
    "analyze:bundle": "node scripts/analyze-bundle.cjs",
    "analyze:deps": "npx depcheck && npm audit",
    "analyze:size": "npm run analyze:bundle",
    "analyze:ci": "node scripts/analyze-bundle.cjs && node scripts/performance-budget.cjs",
    "performance:budget": "node scripts/performance-budget.js",
    "performance:monitor": "node scripts/performance-monitor.js",
    "performance:lighthouse": "node scripts/performance-monitor.js --lighthouse",
    "performance:full": "npm run performance:monitor && npm run performance:lighthouse",
    "test:optimizations": "powershell -ExecutionPolicy Bypass -File scripts/test-optimizations.ps1",
    "_comment_security": "Security & Compliance Scripts",
    "security:scan": "npx snyk test --severity-threshold=medium",
    "security:code": "npx snyk code test --severity-threshold=high",
    "security:monitor": "node scripts/security-monitoring.js",
    "security:all": "npm run security:scan && npm run security:code && npm run license:check && npm run security:monitor",
    "security:dashboard": "node scripts/security-dashboard.js",
    "license:check": "node scripts/license-compliance.js",
    "compliance:check": "npm run license:check && npm run security:monitor",
    "_comment_monitoring": "Advanced Monitoring & Analytics Scripts",
    "performance:monitoring": "node scripts/performance-monitoring.js",
    "alerting:check": "node scripts/alerting-system.js",
    "monitoring:full": "npm run performance:monitoring && npm run alerting:check",
    "lighthouse:ci": "lhci autorun",
    "monitoring:dashboard": "npm run security:dashboard && npm run performance:monitoring",
    "_comment_workflow": "Development Workflow Scripts",
    "start": "npm run dev",
    "start:prod": "npm run build && npm run preview",
    "workflow": "powershell -ExecutionPolicy Bypass -File scripts/dev-workflow.ps1",
    "precommit": "node scripts/pre-commit-hook.ts",
    "precommit:fix": "node scripts/pre-commit-hook.ts --fix",
    "prepare-release": "powershell -ExecutionPolicy Bypass -File scripts/release-prep.ps1",
    "build:optimize": "powershell -ExecutionPolicy Bypass -File scripts/build-optimization-clean.ps1",
    "build:analyze": "powershell -ExecutionPolicy Bypass -File scripts/build-optimization-clean.ps1 -Analyze",
    "predev": "npm run check",
    "_comment_deployment": "Deployment Scripts",
    "deploy:preview": "npm run build && npx wrangler pages deploy dist --project-name premium-weather-app",
    "deploy:production": "npm run build && npx wrangler pages deploy dist --project-name premium-weather-app --branch main",
    "deploy:dev": "npm run build && npx wrangler pages deploy dist --project-name premium-weather-app-dev",
    "deploy:test": "npm run build:ultra && npx wrangler pages deploy dist --project-name premium-weather-app",
    "deploy:diagnostic": "node scripts/cloudflare-diagnostic.js",
    "deploy:domains": "node scripts/setup-custom-domains.js",
    "predeploy:production": "npm run qa",
    "_comment_mobile": "Mobile Development Scripts",
    "streamline": "node scripts/streamline-package-scripts.ts",
    "streamline:dry": "node scripts/streamline-package-scripts.ts --dry-run",
    "workflows:cleanup": "node scripts/cleanup-redundant-workflows.ts",
    "workflows:cleanup:dry": "node scripts/cleanup-redundant-workflows.ts --dry-run",
    "mobile:init": "npx cap init",
    "mobile:build": "npm run build && npx cap sync",
    "mobile:android": "npx cap run android",
    "mobile:ios": "npx cap run ios",
    "mobile:sync": "npx cap sync",
    "mobile:open:android": "npx cap open android",
    "mobile:open:ios": "npx cap open ios",
    "_comment_diagnostic": "Diagnostic & Health Check Scripts",
    "health": "node scripts/health-check.js",
    "health:fix": "node scripts/health-check.js --fix",
    "doctor": "node scripts/dev-doctor.js",
    "doctor:fix": "node scripts/dev-doctor.js --fix",
    "test:apis": "node scripts/test-weather-apis.js",
    "design:validate": "node scripts/validate-ios-design.js",
    "design:fix": "node scripts/validate-ios-design.js --fix",
    "_comment_mobile_deployment": "Enhanced Mobile Deployment Scripts",
    "mobile:deploy:ios": "node scripts/mobile-deploy.js ios debug",
    "mobile:deploy:ios:release": "node scripts/mobile-deploy.js ios release",
    "mobile:deploy:android": "node scripts/mobile-deploy.js android debug",
    "mobile:deploy:android:release": "node scripts/mobile-deploy.js android release",
    "mobile:deploy:ios:run": "node scripts/mobile-deploy.js ios debug --run",
    "mobile:deploy:android:run": "node scripts/mobile-deploy.js android debug --run",
    "mobile:deploy:ios:open": "node scripts/mobile-deploy.js ios debug --open",
    "mobile:deploy:android:open": "node scripts/mobile-deploy.js android debug --open",
    "mobile:doctor": "npm run doctor && npm run test:apis && npm run design:validate",
    "_comment_utility": "Utility & Helper Scripts",
    "scripts:list": "node -e \"const pkg = require('./package.json'); console.log('ðŸ“‹ Available Scripts:'); Object.keys(pkg.scripts).filter(s => !s.startsWith('_')).forEach(s => console.log('  npm run', s));\"",
    "scripts:demo": "node scripts/demo-summary.js",
    "_comment_setup": "Quick Setup & Onboarding Scripts",
    "setup": "node scripts/quick-setup.js",
    "setup:start": "node scripts/quick-setup.js --start",
    "onboard": "npm run setup:start",
    "_comment_complete_workflow": "Complete Development Workflow",
    "dev:setup": "npm run doctor:fix && npm run health:fix && npm run test:apis",
    "dev:check": "npm run health && npm run test:apis && npm run design:validate",
    "dev:full": "npm run dev:setup && npm run dev && npm run test:watch",
    "_comment_release": "Release Management Scripts",
    "prerelease": "npm run qa && npm run analyze",
    "release:patch": "npm version patch && npm run deploy:production",
    "release:minor": "npm version minor && npm run deploy:production",
    "release:major": "npm version major && npm run deploy:production",
    "postversion": "git push && git push --tags",
    "ci:health": "npm run health && npm run test:apis",
    "ci:deploy": "npm run build && npm run performance:monitor"
  },
  "dependencies": {
    "@capacitor/app": "^7.0.1",
    "@capacitor/device": "^7.0.1",
    "@capacitor/geolocation": "^7.1.4",
    "@capacitor/haptics": "^7.0.1",
    "@capacitor/local-notifications": "^7.0.1",
    "@capacitor/network": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@dash0/sdk-web": "^0.13.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@eslint/js": "^9.34.0",
    "@lhci/cli": "^0.15.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/axios": "^0.14.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^10.0.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "jsdom": "^26.1.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.20.0",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.1.3",
  "vitest": "^3.2.4",
  "prettier": "^3.3.3",
    "wrangler": "^4.25.0"
  },
  "overrides": {
    "rollup": "^4.20.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.20.0",
    "@rollup/rollup-darwin-x64": "^4.20.0",
    "@rollup/rollup-win32-x64-msvc": "^4.20.0"
  }
}
