{
  "name": "premium-weather-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Premium weather app with dark/light themes, animated SVG icons, and comprehensive forecasting",
  "keywords": [
    "weather",
    "react",
    "typescript",
    "theme",
    "forecast",
    "animated-icons"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npx tsc -b && npx vite build",
    "build:production": "NODE_ENV=production npx tsc -b && npx vite build --mode production",
    "build:analyze": "tsc -b && vite build --mode production && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "deploy:preview": "npm run build && npx wrangler pages publish dist --project-name premium-weather-app --compatibility-date 2024-01-01",
    "deploy:production": "npm run build:production && npx wrangler pages publish dist --project-name premium-weather-app --compatibility-date 2024-01-01",
    "mobile:setup": "powershell -ExecutionPolicy Bypass -File scripts/setup-mobile-deployment.ps1",
    "mobile:init": "npx cap init",
    "mobile:build": "npm run build && npx cap sync",
    "mobile:android": "npx cap run android",
    "mobile:ios": "npx cap run ios",
    "mobile:sync": "npx cap sync",
    "mobile:open:android": "npx cap open android",
    "mobile:open:ios": "npx cap open ios"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/app": "^6.0.1",
    "@capacitor/device": "^6.0.1",
    "@capacitor/geolocation": "^6.0.1",
    "@capacitor/haptics": "^6.0.1",
    "@capacitor/ios": "^6.1.2",
    "@capacitor/keyboard": "^6.0.2",
    "@capacitor/local-notifications": "^6.1.0",
    "@capacitor/network": "^6.0.2",
    "@capacitor/push-notifications": "^6.0.2",
    "@capacitor/share": "^6.0.2",
    "@capacitor/splash-screen": "^6.0.2",
    "@capacitor/status-bar": "^6.0.1",
    "axios": "^1.10.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.2.1",
    "@capacitor/core": "^6.2.1",
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/axios": "^0.14.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "jsdom": "^26.1.0",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.25.0"
  }
}
