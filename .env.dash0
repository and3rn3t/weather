# Dash0 Configuration for Weather App
# Copy this file to .env.local and replace with your actual Dash0 credentials

# Required: Your Dash0 endpoint URL (provided by Dash0)
VITE_DASH0_ENDPOINT_URL=https://ingress.dash0.dev

# Required: Your Dash0 authorization token (format: auth_xxxxx...)
# Get this from your Dash0 dashboard
VITE_DASH0_AUTH_TOKEN=your-dash0-auth-token-here

# Optional: Dataset name (defaults to "default")
# VITE_DASH0_DATASET=production

# Example configuration (replace with your actual values):
# VITE_DASH0_ENDPOINT_URL=https://ingress.dash0.dev
# VITE_DASH0_AUTH_TOKEN=auth_abc123def456ghi789jkl012mno345pqr678stu901vwx234yz
# VITE_DASH0_DATASET=weather-app-prod
