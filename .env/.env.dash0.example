# Dash0 Environment Configuration
# Copy this to .env.local and update with your actual Dash0 credentials

# Dash0 Authentication Token (required)
# Get this from your Dash0 dashboard: https://app.dash0.com
VITE_DASH0_AUTH_TOKEN=your-dash0-auth-token-here

# Dash0 Endpoint (optional - uses default if not specified)
# This is typically provided in your Dash0 organization settings
VITE_DASH0_ENDPOINT=https://ingress.dash0.com/v1/traces

# Environment (optional - defaults to NODE_ENV)
VITE_DASH0_ENVIRONMENT=development

# Service Name (optional - defaults to weather)
VITE_DASH0_SERVICE_NAME=weather

# Service Version (optional - defaults to 1.0.0)
VITE_DASH0_SERVICE_VERSION=1.0.0

# Enable/Disable Dash0 Telemetry (optional - defaults to true)
VITE_DASH0_ENABLED=true

# Debug Mode (optional - enables verbose logging)
VITE_DASH0_DEBUG=false
